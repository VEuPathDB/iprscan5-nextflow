profiles {
    interpro {
        params {
            input = "$baseDir/data/large.fa"
            outputDir = "$launchDir/output"
            fastaSubsetSize = 10000
            appls = "pfam"
            outputFile = "iprscan_out.tsv"
            abbrev = "haraEmoy2"
            taxonId = 272952
        }
        docker {
            enabled = true
            runOptions = "-v $baseDir/data/downloadedData:/opt/interproscan/data"
        }
    }
    arba {
        params {
            interproResults = "$baseDir/temp/iprscan/"
            proteomes = "$baseDir/temp/fastas/"
            outputDir = "$launchDir/output"
            taxonIdFile = "$baseDir/temp/taxonIdFile.tsv"
        }
        docker {
            enabled = true
        }
    }
}