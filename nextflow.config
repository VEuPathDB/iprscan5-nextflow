profiles {
    interpro {
        params {
            input = "$baseDir/data/large.fa"
            outputDir = "$launchDir/output"
            fastaSubsetSize = 10000
            appls = "pfam"
            outputFile = "iprscan_out.tsv"
            abbrev = "tgonME49"
            taxonId = 508771
        }
        docker {
            enabled = true
            runOptions = "-v $baseDir/data/downloadedData:/opt/interproscan/data"
        }
    }
    arba {
        params {
            interproResults = "$baseDir/data/haraEmoy_iprscan.tsv"
            outputDir = "$launchDir/output"
            taxonId = 272952
        }
        docker {
            enabled = true
        }
    }
}